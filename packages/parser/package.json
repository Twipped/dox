{
  "name": "@jsdox/parser",
  "description": "JSDoc Parser",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Jocelyn Badgley <joc@twipped.com>",
  "homepage": "https://github.com/twipped/dox",
  "repository": {
    "type": "git",
    "url": "git://github.com/twipped/dox.git"
  },
  "keywords": [
    "documentation",
    "docs",
    "markdown",
    "jsdoc"
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "lint-fix": "eslint --fix ."
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "type": "module",
  "main": "./index.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib",
    "index.js"
  ],
  "dependencies": {
    "@babel/generator": "^7.17.9",
    "@babel/parser": "^7.17.9",
    "@es-joy/jsdoccomment": "^0.29.0",
    "comment-parser": "^1.3.1",
    "debug": "^4.0.0",
    "enhanced-resolve": "^5.10.0",
    "jsdoctypeparser": "^9.0.0",
    "lodash": "^4.0.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.18.6",
    "@twipped/eslint-config": "^7.4.0",
    "@twipped/jest-resolver": "^1.0.0",
    "babel-jest": "^28.1.3",
    "babel-plugin-transform-import-meta": "^2.2.0",
    "eslint": "^8.14.0",
    "fast-glob": "^3.2.12",
    "jest": "^28.1.3"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/?(*.)+(spec|test).js"
    ],
    "transform": {
      "\\.js$": "babel-jest"
    },
    "transformIgnorePatterns": [],
    "resolver": "@twipped/jest-resolver",
    "automock": false,
    "clearMocks": true,
    "collectCoverage": false
  }
}
